#include<stdio.h>
int main()
{
    int n,a,b,c,d,e;
    int t,sum=0;

    scanf("%d",&t);

    for(int j=1; j<=t; j++)
    {
        scanf("%d",&n);

        e=n%10;
        n=n/10;
        d=n%10;
        n=n/10;
        c=n%10;
        n=n/10;
        b=n%10;
        n=n/10;
        a=n%10;

        sum=a+e;
        printf("Sum = %d\n",sum);
    }
}
