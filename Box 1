#include<stdio.h>
int main()
{
    int T,k;
    scanf("%d",&T);

    for(k=1; k<=T; k++)
    {
        int a;
        scanf("%d",&a);

        for(int i=1; i<=a; i++)
        {
            for(int j=1; j<=a; j++)
            {
                printf("*");
            }
            printf("\n");

        }
        if(T!=k)
        {
            printf("\n");
        }
    }
}
