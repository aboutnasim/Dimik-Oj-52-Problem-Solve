#include<stdio.h>
int main()
{
    int t,n;
    int a[1001];

    scanf("%d",&t);
    for(int i=1; i<=t; i++)
    {
        scanf("%d",&n);
        for(int j=0; j<n; j++)
        {
            scanf("%d",&a[j]);
        }

        printf("%d",a[0]);
        for(int j=2; j<n; j=j+2)
        {
            printf(" %d",a[j]);            
        }
        printf("\n");
    }
}
